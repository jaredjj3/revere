generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SquozeResponse {
  id             Int      @id @default(autoincrement())
  createdAt      DateTime @default(now())
  httpStatusCode Int
  header         String?
  errorMessage   String?
}

model Schedule {
  id             Int     @id @default(autoincrement())
  name           String  @unique
  description    String?
  command        String
  cronExpression String
  active         Boolean
}
