{
  "name": "revere",
  "description": "notification service",
  "version": "1.1.0",
  "author": "Jared Johnson <jared@jaredjohnson.dev>",
  "repository": "git@github.com:jaredjj3/revere.git",
  "types": "lib/index.d.ts",
  "scripts": {
    "typecheck": "yarn tsc --noEmit",
    "build": "docker-compose build --build-arg GIT_COMMIT_HASH=$(git rev-parse HEAD) --build-arg GIT_COMMIT_STATUS=$((git diff-index --quiet HEAD -- && echo CLEAN) || echo DIRTY)",
    "up": "yarn build && docker-compose up",
    "down": "docker-compose down",
    "docker": "yarn build && docker-compose run --rm listener",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "revere": "bin/run",
    "setup": "cp -n templates/template.env .env || echo '.env already exists'",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "bugs": "https://github.com/jaredjj3/revere/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@prisma/client": "^2.16.0",
    "@types/lodash": "^4.14.168",
    "discord.js": "^12.5.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "global": "^4.4.0",
    "inversify": "^5.0.5",
    "lodash": "^4.17.20",
    "node-cron": "^2.0.3",
    "node-html-parser": "^2.1.0",
    "numeral": "^2.0.6",
    "pm2": "^4.5.4",
    "reflect-metadata": "^0.1.13",
    "tslib": "^1",
    "winston": "^3.3.3",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/inversify": "^2.0.33",
    "@types/node": "^10",
    "@types/node-cron": "^2.0.3",
    "@types/numeral": "^2.0.0",
    "@types/winston": "^2.4.4",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "globby": "^10",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prisma": "2.16.0",
    "ts-node": "^8",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/git@github.com:jaredjj3/revere.git",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.ts",
  "oclif": {
    "commands": "./lib/oclif/commands/",
    "bin": "revere",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "bin": {
    "revere": "./bin/run"
  }
}
